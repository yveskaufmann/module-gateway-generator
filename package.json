{
  "name": "module-gateway-generator",
  "displayName": "module-gateway-generator",
  "description": "This vscode extension allows you generate index.ts files which re export all local files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "typescript"
  ],
  "activationEvents": [
    "onCommand:module-gateway-generator.generate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "module-gateway-generator.generate",
        "title": "Generate module gateway"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.5.3",
    "vscode-test": "^1.0.2"
  }
}
